generator client {
  provider = "prisma-client-js"
  output   = "../src/generated/prisma"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}


model Comment {
  id        Int      @id @default(autoincrement())
  category  String
  text      String
  createdAt DateTime @default(now())
}

